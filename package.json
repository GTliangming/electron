{
  "name": "myelectronapp",
  "version": "1.0.0",
  "description": "electron +  react +ts项目",
  "main": "build/index.js",
  "scripts": {
    "start-electron": "NODE_ENV=development electron .",
    "start": "electron .",
    "dev:renderer": "cross-env NODE_ENV=development webpack -p --config webpack.renderer.config.js --env=dev --debug --watch --progress --colors",
    "build-render": "cross-env NODE_ENV=production webpack -p --config webpack.renderer.config.js",
    "build-main": "webpack  --config webpack.main.config.js",
    "build": "npm install && npm run build-render && npm run build-main && npm run build-electron"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GTliangming/electron.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/GTliangming/electron/issues"
  },
  "homepage": "https://github.com/GTliangming/electron#readme",
  "dependencies": {
    "antd": "^3.23.2",
    "glob": "^7.1.4",
    "npm": "^7.19.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.6.0",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^4.3.5",
    "autoprefixer": "^9.6.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.12.1",
    "cross-env": "^5.2.1",
    "css-loader": "^3.2.0",
    "electron": "11.0.0",
    "nodemon": "^2.0.9",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.1.2",
    "postcss-simple-vars": "^5.0.2",
    "style-loader": "^1.0.0",
    "thread-loader": "^2.1.3",
    "typescript": "^3.6.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.8"
  }  
}
